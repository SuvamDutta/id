class Repository:
    def __init__(self):
        self.data = []

    def add_item(self, item):
        self.data.append(item)

    def remove_item(self, item):
        self.data.remove(item)

    def get_all_items(self):
        return self.data

# Example usage
repo = Repository()

repo.add_item("Item 1")
repo.add_item("Item 2")
repo.add_item("Item 3")

print(repo.get_all_items())

repo.remove_item("Item 2")

print(repo.get_all_items())
